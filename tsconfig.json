{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["node"],
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "Root"
  ]
}
